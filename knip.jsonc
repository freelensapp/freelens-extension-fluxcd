{
  "$schema": "https://unpkg.com/knip@5/schema-jsonc.json",

  "entry": ["out/{main,preload}/index.js!", "src/{main,preload}/index.ts"],
  "project": ["*.{js,mjs,ts}", "out/**/*.js!", "src/**/*.{ts,tsx}"],

  "ignoreBinaries": ["build:.*"],
  "ignoreDependencies": ["@freelensapp/extensions", "electron", "react", "@types/react"],

  "typescript": {
    "config": ["tsconfig.json"]
  },
  "vite": {
    "config": ["electron.vite.config.ts"]
  },

  "exclude": ["exports"]
}
